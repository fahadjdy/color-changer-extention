{
  "manifest_version": 2,
  "name": "Color Changer",
  "version": "1.0",
  "description": "Change the background color of webpages",
  "permissions": ["activeTab"],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png"
    }
  },
  "icons": {
    "16": "icon16.png"
  }
}
